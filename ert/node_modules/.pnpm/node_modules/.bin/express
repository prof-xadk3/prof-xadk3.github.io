#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/express@3.21.2/node_modules/express/bin/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/express@3.21.2/node_modules/express/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/express@3.21.2/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/express@3.21.2/node_modules/express/bin/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/express@3.21.2/node_modules/express/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/express@3.21.2/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../express/bin/express" "$@"
else
  exec node  "$basedir/../express/bin/express" "$@"
fi
