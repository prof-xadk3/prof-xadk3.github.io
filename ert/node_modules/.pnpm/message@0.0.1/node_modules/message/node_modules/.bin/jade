#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/jade/bin/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/jade/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules:/home/nick/FAMMstack/portfolio/node_modules:/home/nick/FAMMstack/node_modules:/home/nick/node_modules:/home/node_modules:/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/jade/bin/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/jade/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules:/home/nick/FAMMstack/portfolio/node_modules:/home/nick/FAMMstack/node_modules:/home/nick/node_modules:/home/node_modules:/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jade/bin/jade" "$@"
else
  exec node  "$basedir/../jade/bin/jade" "$@"
fi
