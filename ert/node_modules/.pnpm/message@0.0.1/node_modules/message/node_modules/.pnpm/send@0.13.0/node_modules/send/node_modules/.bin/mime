#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/.pnpm/mime@1.3.4/node_modules/mime/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/.pnpm/mime@1.3.4/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/.pnpm/mime@1.3.4/node_modules/mime/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/.pnpm/mime@1.3.4/node_modules:/home/nick/FAMMstack/portfolio/ert/node_modules/.pnpm/message@0.0.1/node_modules/message/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@1.3.4/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@1.3.4/node_modules/mime/cli.js" "$@"
fi
