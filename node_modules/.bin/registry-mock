#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules/@pnpm/registry-mock/dist/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules/@pnpm/registry-mock/dist/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules/@pnpm/registry-mock/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules/@pnpm/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules/@pnpm/registry-mock/dist/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules/@pnpm/registry-mock/dist/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules/@pnpm/registry-mock/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules/@pnpm/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/@pnpm+registry-mock@3.8.0/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@pnpm/registry-mock/dist/bin/pnpm-registry-mock.js" "$@"
else
  exec node  "$basedir/../@pnpm/registry-mock/dist/bin/pnpm-registry-mock.js" "$@"
fi
