#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/phonegap@9.0.0/node_modules/phonegap/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/phonegap@9.0.0/node_modules/phonegap/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/phonegap@9.0.0/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/phonegap@9.0.0/node_modules/phonegap/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/phonegap@9.0.0/node_modules/phonegap/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/phonegap@9.0.0/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../phonegap/bin/phonegap.js" "$@"
else
  exec node  "$basedir/../phonegap/bin/phonegap.js" "$@"
fi
