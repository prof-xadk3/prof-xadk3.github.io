#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/webhook-client@0.4.17/node_modules/webhook-client/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/webhook-client@0.4.17/node_modules/webhook-client/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/webhook-client@0.4.17/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/webhook-client@0.4.17/node_modules/webhook-client/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/webhook-client@0.4.17/node_modules/webhook-client/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/webhook-client@0.4.17/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webhook-client/bin/webhook-client" "$@"
else
  exec node  "$basedir/../webhook-client/bin/webhook-client" "$@"
fi
