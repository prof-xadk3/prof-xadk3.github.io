#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cbor-extract@2.1.1/node_modules/cbor-extract/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cbor-extract@2.1.1/node_modules/cbor-extract/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cbor-extract@2.1.1/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cbor-extract@2.1.1/node_modules/cbor-extract/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cbor-extract@2.1.1/node_modules/cbor-extract/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cbor-extract@2.1.1/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cbor-extract@2.1.1/node_modules/cbor-extract/bin/download-prebuilds.js" "$@"
else
  exec node  "$basedir/../../../../../cbor-extract@2.1.1/node_modules/cbor-extract/bin/download-prebuilds.js" "$@"
fi
