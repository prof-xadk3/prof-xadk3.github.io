#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/localtunnel@1.9.2/node_modules/localtunnel/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/localtunnel@1.9.2/node_modules/localtunnel/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/localtunnel@1.9.2/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/localtunnel@1.9.2/node_modules/localtunnel/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/localtunnel@1.9.2/node_modules/localtunnel/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/localtunnel@1.9.2/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../localtunnel/bin/client" "$@"
else
  exec node  "$basedir/../localtunnel/bin/client" "$@"
fi
