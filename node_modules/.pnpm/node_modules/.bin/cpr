#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cpr@3.0.1/node_modules/cpr/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cpr@3.0.1/node_modules/cpr/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cpr@3.0.1/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cpr@3.0.1/node_modules/cpr/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cpr@3.0.1/node_modules/cpr/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/cpr@3.0.1/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cpr/bin/cpr" "$@"
else
  exec node  "$basedir/../cpr/bin/cpr" "$@"
fi
