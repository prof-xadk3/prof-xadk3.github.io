#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/verdaccio@5.23.1/node_modules/verdaccio/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/verdaccio@5.23.1/node_modules/verdaccio/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/verdaccio@5.23.1/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/verdaccio@5.23.1/node_modules/verdaccio/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/verdaccio@5.23.1/node_modules/verdaccio/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/verdaccio@5.23.1/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../verdaccio/bin/verdaccio" "$@"
else
  exec node  "$basedir/../verdaccio/bin/verdaccio" "$@"
fi
