#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/ometajs@3.2.4/node_modules/ometajs/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/ometajs@3.2.4/node_modules/ometajs/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/ometajs@3.2.4/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/ometajs@3.2.4/node_modules/ometajs/bin/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/ometajs@3.2.4/node_modules/ometajs/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/ometajs@3.2.4/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ometajs/bin/ometajs2js" "$@"
else
  exec node  "$basedir/../ometajs/bin/ometajs2js" "$@"
fi
