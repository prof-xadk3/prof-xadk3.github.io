#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/react-native@0.71.4_4f7gtdna7t4us42uriwkylygoi/node_modules/react-native/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/react-native@0.71.4_4f7gtdna7t4us42uriwkylygoi/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/FAMMstack/portfolio/node_modules/.pnpm/react-native@0.71.4_4f7gtdna7t4us42uriwkylygoi/node_modules/react-native/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/react-native@0.71.4_4f7gtdna7t4us42uriwkylygoi/node_modules:/home/nick/FAMMstack/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
